                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_17pxv"
                    android:paddingStart="@dimen/_19pxh"
                    android:paddingTop="@dimen/_17pxv"
                    android:singleLine="true"
                    android:text="@={addAddressVM.addAddressModel.etGroup99Value}"
                    android:textColorHint="@color/bluegray_300" style="@style/etRoundedOutline"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtStateProvince"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{addAddressVM.addAddressModel.txtStateProvince}"
                    style="@style/txtPoppinsbold14_2" tools:text="@string/msg_state_province"/>
                <EditText
                    android:background="@drawable/rectangle_border_blue_50_radius_5"
                    android:ellipsize="end"
                    android:hint="@string/msg_enter_the_state"
                    android:id="@+id/etGroup100"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_17pxv"
                    android:paddingStart="@dimen/_19pxh"
                    android:paddingTop="@dimen/_17pxv"
                    android:singleLine="true"
                    android:text="@={addAddressVM.addAddressModel.etGroup100Value}"
                    android:textColorHint="@color/bluegray_300" style="@style/etRoundedOutline"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtZipCode"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{addAddressVM.addAddressModel.txtZipCode}"
                    style="@style/txtPoppinsbold14_2" tools:text="@string/lbl_zip_code"/>
                <EditText
                    android:background="@drawable/rectangle_border_blue_50_radius_5"
                    android:ellipsize="end"
                    android:hint="@string/msg_enter_the_zip_c"
                    android:id="@+id/etGroup101"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_17pxv"
                    android:paddingStart="@dimen/_19pxh"
                    android:paddingTop="@dimen/_17pxv"
                    android:singleLine="true"
                    android:text="@={addAddressVM.addAddressModel.etGroup101Value}"
                    android:textColorHint="@color/bluegray_300" style="@style/etRoundedOutline"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPhoneNumber"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{addAddressVM.addAddressModel.txtPhoneNumber}"
                    style="@style/txtPoppinsbold14_2" tools:text="@string/lbl_phone_number"/>
                <EditText
                    android:background="@drawable/rectangle_border_blue_50_radius_5"
                    android:ellipsize="end"
                    android:hint="@string/msg_enter_the_phone"
                    android:id="@+id/etGroup102"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_17pxv"
                    android:paddingStart="@dimen/_19pxh"
                    android:paddingTop="@dimen/_17pxv"
                    android:singleLine="true"
                    android:text="@={addAddressVM.addAddressModel.etGroup102Value}"
                    android:textColorHint="@color/bluegray_300" style="@style/etRoundedOutline"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnAddAddress"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_16pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_16pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_add_address"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded" tools:text="@string/lbl_add_address"/>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
