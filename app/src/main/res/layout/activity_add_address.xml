                <TextView android:ellipsize="end"
                    android:id="@+id/txtFirstName"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{addAddressVM.addAddressModel.txtFirstName}"
                    style="@style/txtPoppinsbold14_2" tools:text="@string/lbl_first_name"/>
                <EditText
                    android:background="@drawable/rectangle_border_blue_50_radius_5"
                    android:ellipsize="end"
                    android:hint="@string/msg_enter_the_first"
                    android:id="@+id/etGroup103"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_17pxv"
                    android:paddingStart="@dimen/_19pxh"
                    android:paddingTop="@dimen/_17pxv"
                    android:singleLine="true"
                    android:text="@={addAddressVM.addAddressModel.etGroup103Value}"
                    android:textColorHint="@color/bluegray_300" style="@style/etRoundedOutline"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLastName"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_23pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{addAddressVM.addAddressModel.txtLastName}"
                    style="@style/txtPoppinsbold14_2" tools:text="@string/lbl_last_name"/>
                <EditText
