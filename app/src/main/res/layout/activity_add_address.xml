                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_17pxv"
                    android:paddingStart="@dimen/_19pxh"
                    android:paddingTop="@dimen/_17pxv"
                    android:singleLine="true"
                    android:text="@={addAddressVM.addAddressModel.etGroup96Value}"
                    android:textColorHint="@color/bluegray_300" style="@style/etRoundedOutline"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtStreetAddress"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{addAddressVM.addAddressModel.txtStreetAddress}"
                    style="@style/txtPoppinsbold14_2" tools:text="@string/lbl_street_address"/>
                <EditText
                    android:background="@drawable/rectangle_border_blue_50_radius_5"
                    android:ellipsize="end"
                    android:hint="@string/msg_enter_the_stree"
                    android:id="@+id/etGroup97"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_17pxv"
                    android:paddingStart="@dimen/_19pxh"
                    android:paddingTop="@dimen/_17pxv"
                    android:singleLine="true"
                    android:text="@={addAddressVM.addAddressModel.etGroup97Value}"
                    android:textColorHint="@color/bluegray_300" style="@style/etRoundedOutline"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtStreetAddress1"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_22pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{addAddressVM.addAddressModel.txtStreetAddress1}"
                    style="@style/txtPoppinsbold14_2" tools:text="@string/msg_street_address"/>
                <EditText
                    android:background="@drawable/rectangle_border_blue_50_radius_5"
                    android:ellipsize="end"
                    android:hint="@string/msg_enter_the_stree2"
                    android:id="@+id/etGroup98"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_17pxv"
                    android:paddingStart="@dimen/_19pxh"
                    android:paddingTop="@dimen/_17pxv"
                    android:singleLine="true"
                    android:text="@={addAddressVM.addAddressModel.etGroup98Value}"
