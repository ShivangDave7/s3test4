<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="writeReviewFillVM" type="com.isapplication.app.modules.writereviewfill.data.viewmodel.WriteReviewFillVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearWriteReviewFi"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearGroup487"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_26pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearGroup220"
                    android:layout_height="match_parent"
                    android:layout_marginEnd="@dimen/_194pxh"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageLefticon"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_lefticon"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtWriteReview"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_weight="0.82"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{writeReviewFillVM.writeReviewFillModel.txtWriteReview}"
                        style="@style/txtPoppinsbold16" tools:text="@string/lbl_write_review"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <TextView android:id="@+id/txtPleasewriteOv"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_44pxv"
                android:layout_width="match_parent"
                android:text="@{writeReviewFillVM.writeReviewFillModel.txtPleasewriteOv}"
                style="@style/txtPoppinsbold14_2" tools:text="@string/msg_please_write_ov"/>
            <androidx.appcompat.widget.AppCompatRatingBar
                android:id="@+id/ratingBarRatingbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="wrap_content" android:numStars="5"
                android:progressBackgroundTint="@color/blue_50"
                android:progressTint="@color/yellow_700"
                android:rating="4" style="@style/Widget.AppCompat.RatingBar.Indicator"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtWriteYourRevi"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{writeReviewFillVM.writeReviewFillModel.txtWriteYourRevi}"
                style="@style/txtPoppinsbold14_2" tools:text="@string/msg_write_your_revi"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_blue_50_radius_5"
                android:ellipsize="end"
                android:hint="@string/lbl_add_review"
                android:id="@+id/etGroup31"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_34pxv"
                android:paddingEnd="@dimen/_16pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_20pxv"
                android:singleLine="true"
                android:text="@={writeReviewFillVM.writeReviewFillModel.etGroup31Value}"
                android:textColorHint="@color/bluegray_300" style="@style/etSolidRoundedOutline_1"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnWriteReview"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_276pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@string/lbl_write_review"
                android:textAllCaps="false"
                style="@style/btnSolidRounded" tools:text="@string/lbl_write_review"/>
        </LinearLayout>
    </LinearLayout>
</layout>
