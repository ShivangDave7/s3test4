<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="searchResult2VM" type="com.isapplication.app.modules.searchresult2.data.viewmodel.SearchResult2VM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearSearchResult"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearGroup287"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearGroup281"
                    android:layout_height="match_parent"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <EditText
                        android:background="@drawable/rectangle_border_blue_50_radius_5"
                        android:drawablePadding="@dimen/_10pxh"
                        android:drawableStart="@drawable/img_searchicon"
                        android:ellipsize="end"
                        android:hint="@string/lbl_search_product"
                        android:id="@+id/etGroup47"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.85"
                        android:layout_width="0dp"
                        android:paddingBottom="@dimen/_19pxv"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_19pxv"
                        android:singleLine="true"
                        android:text="@={searchResult2VM.searchResult2Model.etGroup47Value}"
                        android:textColorHint="@color/bluegray_300" style="@style/etRoundedOutline"/>
                    <ImageView android:id="@+id/imageShorticon"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_marginBottom="@dimen/_11pxv"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_shorticon"/>
                    <ImageView android:id="@+id/imageFiltericon"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_marginBottom="@dimen/_11pxv"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_filter"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <View android:background="@color/blue_50"
                android:id="@+id/lineLine39"
                android:layout_gravity="start"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_375pxh"/>
            <LinearLayout android:id="@+id/linearGroup282"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:alpha="0.5" android:ellipsize="end"
                    android:id="@+id/txtResultCounter"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_weight="0.34"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{searchResult2VM.searchResult2Model.txtResultCounter}"
                    style="@style/txtPoppinsbold12_4" tools:text="@string/lbl_0_result"/>
                <LinearLayout android:id="@+id/linearGroup289"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_187pxh"
                    android:layout_weight="0.66"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtManShoes"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.75"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{searchResult2VM.searchResult2Model.txtManShoes}"
                        style="@style/txtPoppinsbold12_2" tools:text="@string/lbl_man_shoes"/>
                    <ImageView android:id="@+id/imageBottomicon"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_downicon"/>
                </LinearLayout>
            </LinearLayout>
            <ImageView android:id="@+id/imageNotFoundIcon"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_72pxh"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_131pxv"
                android:layout_width="@dimen/_72pxh"
                android:scaleType="fitXY" android:src="@drawable/img_notfoundicon"/>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtProductNotFou"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{searchResult2VM.searchResult2Model.txtProductNotFou}"
                style="@style/txtPoppinsbold24_1" tools:text="@string/msg_product_not_fou"/>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtThankyoufors"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{searchResult2VM.searchResult2Model.txtThankyoufors}"
                style="@style/txtPoppinsregular12" tools:text="@string/msg_thank_you_for_s"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnBackToHome"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@string/lbl_back_to_home"
                android:textAllCaps="false"
                style="@style/btnSolidRounded" tools:text="@string/lbl_back_to_home"/>
        </LinearLayout>
    </LinearLayout>
</layout>
