<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="profile1RowModel" type="com.isapplication.app.modules.profile.data.model.Profile1RowModel"/>
    </data>
    <LinearLayout android:clickable="true" android:id="@+id/linearGroup"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearGroup406"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_15pxv"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_weight="0.55" android:layout_width="0dp" android:orientation="horizontal">
            <ImageView android:id="@+id/imageGendericon"
                android:layout_height="@dimen/_24pxh"
                android:layout_width="@dimen/_24pxh"
                android:scaleType="fitXY" android:src="@drawable/img_gendericon"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtGender"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_3pxv"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_weight="0.67" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{profile1RowModel.txtGender}"
                style="@style/txtPoppinsbold12_2" tools:text="@string/lbl_gender"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearGroup407"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_15pxv"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_184pxh"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_weight="0.45" android:layout_width="0dp" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/txtGender1"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.56" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{profile1RowModel.txtGender1}"
                style="@style/txtPoppinsregular12" tools:text="@string/lbl_male"/>
            <ImageView android:id="@+id/imageRighticon"
                android:layout_height="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_width="@dimen/_24pxh"
                android:scaleType="fitXY" android:src="@drawable/img_righticon"/>
        </LinearLayout>
    </LinearLayout>
</layout>
