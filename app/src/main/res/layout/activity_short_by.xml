<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="shortByVM" type="com.isapplication.app.modules.shortby.data.viewmodel.ShortByVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearShortBy"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearGroup310"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginTop="@dimen/_26pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearGroup305"
                    android:layout_height="match_parent"
                    android:layout_marginEnd="@dimen/_247pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageLefticon"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_lefticon"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtShoryBy"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_weight="0.76"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{shortByVM.shortByModel.txtShoryBy}"
                        style="@style/txtPoppinsbold16" tools:text="@string/lbl_shory_by"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <TextView android:id="@+id/txtBestMatch"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_28pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_16pxv"
                android:text="@{shortByVM.shortByModel.txtBestMatch}"
                style="@style/txtSolid_1" tools:text="@string/lbl_best_match"/>
            <TextView android:id="@+id/txtTimeendingso"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_16pxv"
                android:text="@{shortByVM.shortByModel.txtTimeendingso}"
                style="@style/txtSolid_2" tools:text="@string/msg_time_ending_so"/>
            <TextView android:id="@+id/txtTimenewlylis"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_16pxv"
                android:text="@{shortByVM.shortByModel.txtTimenewlylis}"
                style="@style/txtSolid_3" tools:text="@string/msg_time_newly_lis"/>
            <TextView android:id="@+id/txtPriceShippin"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_16pxv"
                android:text="@{shortByVM.shortByModel.txtPriceShippin}"
                style="@style/txtSolid_3" tools:text="@string/msg_price_shippin"/>
            <TextView android:id="@+id/txtPriceShippin1"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_16pxv"
                android:text="@{shortByVM.shortByModel.txtPriceShippin1}"
                style="@style/txtSolid_3" tools:text="@string/msg_price_shippin2"/>
            <TextView android:id="@+id/txtDistanceneare"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_16pxv"
                android:text="@{shortByVM.shortByModel.txtDistanceneare}"
                style="@style/txtSolid_3" tools:text="@string/msg_distance_neare"/>
        </LinearLayout>
    </LinearLayout>
</layout>
