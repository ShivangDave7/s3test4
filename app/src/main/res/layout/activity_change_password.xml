<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="changePasswordVM" type="com.isapplication.app.modules.changepassword.data.viewmodel.ChangePasswordVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearChangePassword"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearGroup418"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginTop="@dimen/_26pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearGroup416"
                    android:layout_height="match_parent"
                    android:layout_marginEnd="@dimen/_166pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageLefticon"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_lefticon"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtChangePassword"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_weight="0.87"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{changePasswordVM.changePasswordModel.txtChangePassword}"
                        style="@style/txtPoppinsbold16" tools:text="@string/lbl_change_password"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <TextView android:ellipsize="end"
                android:id="@+id/txtOldPassword"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_44pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{changePasswordVM.changePasswordModel.txtOldPassword}"
                style="@style/txtPoppinsbold14_2" tools:text="@string/lbl_old_password"/>
            <EditText
                android:background="@drawable/rectangle_border_blue_50_radius_5"
                android:drawablePadding="@dimen/_10pxh"
                android:drawableStart="@drawable/img_lock_icon"
                android:ellipsize="end" android:hint="@string/msg"
                android:id="@+id/etGroup90"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@={changePasswordVM.changePasswordModel.etGroup90Value}"
                android:textColorHint="@color/bluegray_300" style="@style/etRoundedOutline_3"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtNewPassword"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{changePasswordVM.changePasswordModel.txtNewPassword}"
                style="@style/txtPoppinsbold14_2" tools:text="@string/lbl_new_password"/>
            <EditText
                android:background="@drawable/rectangle_border_blue_50_radius_5"
                android:drawablePadding="@dimen/_10pxh"
                android:drawableStart="@drawable/img_lock_icon"
                android:ellipsize="end" android:hint="@string/msg"
                android:id="@+id/etGroup91"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@={changePasswordVM.changePasswordModel.etGroup91Value}"
                android:textColorHint="@color/bluegray_300" style="@style/etRoundedOutline_3"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtNewPasswordAg"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{changePasswordVM.changePasswordModel.txtNewPasswordAg}"
                style="@style/txtPoppinsbold14_2" tools:text="@string/msg_new_password_ag"/>
            <EditText
                android:background="@drawable/rectangle_border_blue_50_radius_5"
                android:drawablePadding="@dimen/_10pxh"
                android:drawableStart="@drawable/img_lock_icon"
                android:ellipsize="end" android:hint="@string/msg"
                android:id="@+id/etGroup92"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@={changePasswordVM.changePasswordModel.etGroup92Value}"
                android:textColorHint="@color/bluegray_300" style="@style/etRoundedOutline_3"/>
            <androidx.appcompat.widget.AppCompatButton
                android:clickable="true" android:ellipsize="end"
                android:gravity="center" android:id="@+id/btnSave"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_276pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@string/lbl_save"
                android:textAllCaps="false"
                style="@style/btnSolidRounded" tools:text="@string/lbl_save"/>
        </LinearLayout>
    </LinearLayout>
</layout>
